{
  "name": "@silvana-one/prover",
  "description": "Silvana Prover",
  "version": "1.0.15",
  "author": "DFST",
  "bugs": "https://github.com/SilvanaOne/silvana-lib/issues",
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "esbuild": "^0.25.5",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    "types": "./dist/node/index.d.ts",
    "node": {
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "browser": "./dist/web/index.js",
    "default": "./dist/web/index.js"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://docs.silvana.one",
  "keywords": [
    "cloud",
    "fungible-token",
    "mina-protocol",
    "silvana",
    "NFT",
    "proving",
    "tokens",
    "typescript"
  ],
  "license": "Apache-2.0",
  "main": "dist/web/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "SilvanaOne/silvana-lib.git",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc --build tsconfig.web.json && node build-cjs.js",
    "generate": "npm run validate && openapi-ts && rm src/client/index.ts && node replace-import.cjs && npm run build",
    "release": "npm publish --access public",
    "validate": "swagger-cli validate open-api.yaml"
  },
  "type": "module",
  "types": "./dist/node/index.d.ts"
}
