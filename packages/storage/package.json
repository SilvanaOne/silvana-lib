{
  "name": "@silvana-one/storage",
  "description": "Silvana Storage Library",
  "version": "1.1.1",
  "author": "DFST",
  "bugs": "https://github.com/SilvanaOne/silvana-lib/issues",
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "esbuild": "^0.25.11",
    "o1js": "2.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    "types": "./dist/node/index.d.ts",
    "node": {
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "browser": "./dist/web/index.js",
    "default": "./dist/web/index.js"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://docs.silvana.one",
  "keywords": [
    "cloud",
    "mina-protocol",
    "silvana",
    "whitelist",
    "proving",
    "storage",
    "typescript"
  ],
  "license": "Apache-2.0",
  "main": "dist/web/index.js",
  "peerDependencies": {
    "o1js": "2.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "SilvanaOne/silvana-lib.git",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc --build tsconfig.web.json && node build-cjs.js",
    "release": "npm publish --access public",
    "test": "NODE_NO_WARNINGS=1 node --loader=ts-node/esm --enable-source-maps --test",
    "test:indexed-map": "npm run test test/indexed-map.test.ts"
  },
  "type": "module",
  "types": "./dist/node/index.d.ts"
}
