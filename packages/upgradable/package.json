{
  "name": "@silvana-one/upgradable",
  "description": "Silvana Upgradable interface for Mina zkApps",
  "version": "1.0.20",
  "author": "DFST",
  "bugs": "https://github.com/SilvanaOne/silvana-lib/issues",
  "dependencies": {
    "mina-signer": "^3.0.7",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@silvana-one/storage": "1.0.20",
    "@types/node": "^24.0.1",
    "esbuild": "^0.25.5",
    "o1js": "^2.6.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    "types": "./dist/node/index.d.ts",
    "node": {
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "browser": "./dist/web/index.js",
    "default": "./dist/web/index.js"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://docs.silvana.one",
  "keywords": [
    "mina-protocol",
    "typescript",
    "Upgradable",
    "zkApps"
  ],
  "license": "Apache-2.0",
  "main": "dist/web/index.js",
  "peerDependencies": {
    "@silvana-one/storage": "1.0.20",
    "o1js": "^2.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "SilvanaOne/silvana-lib.git",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc --build tsconfig.web.json && node build-cjs.js",
    "release": "npm publish --access public"
  },
  "type": "module",
  "types": "./dist/node/index.d.ts"
}
